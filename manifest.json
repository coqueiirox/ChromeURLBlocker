{
    "manifest_version": 2,
    "name": "URL Blocker",
    "description": "Blacklist URLs",
    "version": "1.0",
    "permissions": ["webRequest", "webRequestBlocking", "<all_urls>", "storage", "webNavigation", "activeTab"],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "options_page": "options.html"
}
